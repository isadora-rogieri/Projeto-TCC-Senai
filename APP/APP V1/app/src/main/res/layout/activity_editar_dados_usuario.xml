        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVEditUser" />

    <Button
        android:id="@+id/btEditarDadosUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="#F11212"
        android:text="Salvar Alterações"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edTextEditComplementoUser" />
    <EditText
        android:id="@+id/edTextEditCidadeUser"
        android:layout_width="178dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="372dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#F11212"
