<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroudapp"
    tools:context=".telas.EditarDadosUsuario">

    <View
        android:id="@+id/divider8"
        android:layout_width="415dp"
        android:layout_height="45dp"
        android:background="#F31313"
        android:backgroundTint="#F31313"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider9"
        android:layout_width="415dp"
        android:layout_height="55dp"
        android:background="#F40C0C"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textVEditUser"
        android:layout_width="199dp"
        android:layout_height="31dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="28dp"
        android:gravity="center"
        android:text="Editar Dados"
        android:textColor="#F11212"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider8" />

    <TextView
        android:id="@+id/textVNomeUser"
        android:layout_width="72dp"
        android:layout_height="33dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="28dp"
        android:text="Nome"
        android:textColor="#F11212"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVEditUser" />

    <EditText
        android:id="@+id/edTextEditNomeUser"
        android:layout_width="178dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#F11212"
        android:textColorHint="#F11212"
        app:layout_constraintStart_toEndOf="@+id/textVNomeUser"
        app:layout_constraintTop_toBottomOf="@+id/textVEditUser" />

    <TextView
        android:id="@+id/textVEmailUser"
        android:layout_width="72dp"
        android:layout_height="33dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="72dp"
        android:text="Email"
        android:textColor="#F11212"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVEditUser" />

    <EditText
        android:id="@+id/edTextEditEmailUser"
        android:layout_width="178dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="72dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#F11212"
        android:textColorHint="#F11212"
        app:layout_constraintStart_toEndOf="@+id/textVEmailUser"
        app:layout_constraintTop_toBottomOf="@+id/textVEditUser" />

    <EditText
        android:id="@+id/edTextEditNumeroUser"
        android:layout_width="178dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="220dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#F11212"
        android:textColorHint="#F11212"
        app:layout_constraintStart_toEndOf="@+id/textVNumeroUser"
        app:layout_constraintTop_toBottomOf="@+id/textVEditUser" />

    <EditText
        android:id="@+id/edTextEditEnderecoUser"
        android:layout_width="178dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="172dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#F11212"
        android:textColorHint="#F11212"
        app:layout_constraintStart_toEndOf="@+id/textVEnderecoUser"
        app:layout_constraintTop_toBottomOf="@+id/textVEditUser" />

    <TextView
        android:id="@+id/textVNumeroUser"
        android:layout_width="88dp"
        android:layout_height="36dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="224dp"
