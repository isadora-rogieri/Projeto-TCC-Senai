        android:inputType="textPersonName"
        android:textColor="#F11212"
        android:textColorHint="#F11212"
        app:layout_constraintStart_toEndOf="@+id/textVCepUser"
        app:layout_constraintTop_toBottomOf="@+id/textVEditUser" />

    <TextView
        android:id="@+id/textVCepUser"
        android:layout_width="88dp"
        android:layout_height="36dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="320dp"
        android:text="CEP"
        android:textColor="#F11212"
